{
  "name": "markdown-it-svg-code-copy",
  "version": "1.0.0",
  "description": "Markdown it plugin to copy code part with svg icon",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "private": false,
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage && rm -rf coverage",
    "test:watch": "jest --watch",
    "coveralls": "jest --coverage && coveralls < coverage/lcov.info && rm -rf coverage",
    "prepare": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcalixte/markdown-it-svg-code-copy.git"
  },
  "keywords": [
    "markdown",
    "markdown-it",
    "plugin",
    "code",
    "copy",
    "svg",
    "icon"
  ],
  "author": "Julien Calixte <julienc@bam.tech>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcalixte/markdown-it-svg-code-copy/issues"
  },
  "homepage": "https://github.com/jcalixte/markdown-it-svg-code-copy#readme",
  "dependencies": {
    "clipboard": "^2.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@types/markdown-it": "^12.2.0",
    "@types/node": "^16.6.1",
    "jest": "^27.0.6",
    "markdown-it": "^12.2.0",
    "prettier": "^2.3.2",
    "release-it": "^14.11.5",
    "typescript": "^4.3.5"
  }
}